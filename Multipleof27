import java.util.Scanner;
class Multipleof27 {
public static void main (String [] args) {
Scanner sc = new Scanner(System.in);
System.out.println("Enter a: ");
int a = sc.nextInt();
if (a%27==0){
System.out.println(a+" "+"is a multiple of 27");
}
else {
System.out.println(a+" "+"is not a multiple of 27");
}
}
}
